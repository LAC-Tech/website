{
	"compilerOptions": {
		"target": "ESNext",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
		"checkJs": true,
		"module": "ESNext",
    "moduleResolution": "Node",
		"isolatedModules": true,
    "noEmit": true,
